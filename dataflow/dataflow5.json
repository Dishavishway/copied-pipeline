					"dataset": {
						"referenceName": "cereal_datalake",
						"type": "DatasetReference"
					},
					"name": "source5"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          CustomerID as string,",
				"          Genre as string,",
				"          Age as string,",
				"          {Annual_Income_(k$)} as string,",
				"          Spending_Score as string",
